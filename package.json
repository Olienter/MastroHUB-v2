{
  "name": "mastrohub-v2",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "smoke": "node scripts/smoke.mjs",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
